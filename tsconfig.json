{
    "files": [
        "src/test.ts"
    ],
    "compilerOptions": {
        "declaration": true,
        "sourceMap": true,
        "outDir": "./dist",
        "module": "es6",
        "target": "es5",
        "strict": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "noImplicitThis": true,
        "alwaysStrict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true
    }
}
